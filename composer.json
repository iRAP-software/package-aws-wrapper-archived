{
    "name": "irap/aws-wrapper",
    "type": "library",
    "description": "Package for rapidly developing models that represent mysql tables. Tables must have an id field.",
    "keywords": ["mysql", "mysqli","models"],
    "homepage": "http://www.irap.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "stuart.page@irap.org",
            "homepage": "http://www.irap.org/",
            "role": "Developer"
        }
    ],
    "repositories": [ { "type": "composer", "url": "http://satis.irap-dev.org/" } ],
    "require": {
        "php": ">=5.3.0",
        "irap/core-libs": "1.1.*",
        "aws/aws-sdk-php" : "3.3.*"
    },
    "autoload": {
        "psr-4": {
            "iRAP\\AwsWrapper\\": "",
            "iRAP\\AwsWrapper\\Enums\\": "enums/",
            "iRAP\\AwsWrapper\\Objects\\": "objects/",
            "iRAP\\AwsWrapper\\Requests\\": "requests/",
            "iRAP\\AwsWrapper\\S3\\": "S3/",
            "iRAP\\AwsWrapper\\Ec2\\": "Ec2/"
            
        }
    }
}
